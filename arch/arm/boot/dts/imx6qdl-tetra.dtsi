&ecspi1 {

        fsl,spi-num-chipselects = <1>;
        cs-gpios = <&gpio2 30 0>; 
        pinctrl-names = "default";
        pinctrl-0 = <&pinctrl_ecspi1 &pinctrl_ecspi1_cs>;
        status = "okay";

        flash: 25fl016ki@0 {
                #address-cells = <1>;
                #size-cells = <1>;
                compatible = "spansion,s25fl016k";
                spi-max-frequency = <20000000>;
                reg = <0>;
        };


};

&ecspi3 {

        pinctrl-names = "default";
        pinctrl-0 = <&pinctrl_ecspi3>;
        status = "disabled";

};


&fec {
	pinctrl-names = "default";
